<theme>

	<include>./_inc/systems/metadata-global/_default.xml</include>
	<include>./_inc/systems/metadata-global/${system.theme}.xml</include>

	<variables>
		<spacerImage>./_inc/images/space.png</spacerImage>
		<fontMedium>./_inc/fonts/solheavy.ttf</fontMedium>
		<langLabelTotalGames>Total Games:</langLabelTotalGames>
		<artworkFolder>./_inc/systems/artwork/${system.theme}.webp</artworkFolder>
		<fallbackSystem>./_inc/systems/artwork/_default_sys.webp</fallbackSystem>
		<fallbackGame>./_inc/systems/artwork/_default_game.webp</fallbackGame>
	</variables>

	<!--///	Esquemas de Cores ///-->
	<colorScheme name="light">
		<variables>
			<CorFundoTopo>f4f4f4</CorFundoTopo>
			<CorFundoRodape>ececec</CorFundoRodape>
			<CorFundoBolinhas>ffffff</CorFundoBolinhas>
			<CorLetrasGeral>403a46</CorLetrasGeral>
			<CorLetrasTitulos>403a46</CorLetrasTitulos>
			<FundoOpacidade>0.24</FundoOpacidade>
			<FundoSaturacao>1</FundoSaturacao>
			<CorMiniFundo>ffffff</CorMiniFundo>
			<CorHelpFundo>ffffffcc</CorHelpFundo>
			<CorHelpFundo2>d1cddacc</CorHelpFundo2>
			<CorDeFato>light</CorDeFato>
			<ImagemSeletor>./_inc/images/grid-seletor-light.png</ImagemSeletor>
			<ItemForaOpacidade>0.7</ItemForaOpacidade>
			<ItemForaTonalidade>0.97</ItemForaTonalidade>
		</variables>
    </colorScheme>
	<colorScheme name="dark">
		<variables>
			<CorFundoTopo>262626</CorFundoTopo>
			<CorFundoRodape>161616</CorFundoRodape>
			<CorFundoBolinhas>3c3c3c</CorFundoBolinhas>
			<CorLetrasGeral>403a46</CorLetrasGeral>
			<CorLetrasTitulos>e0e0e0</CorLetrasTitulos>
			<FundoOpacidade>0.12</FundoOpacidade>
			<FundoSaturacao>1</FundoSaturacao>
			<CorMiniFundo>e0e0e0</CorMiniFundo>
			<CorHelpFundo>ffffff44</CorHelpFundo>
			<CorHelpFundo2>d1cdda44</CorHelpFundo2>
			<CorDeFato>dark</CorDeFato>
			<ImagemSeletor>./_inc/images/grid-seletor-dark.png</ImagemSeletor>
			<ItemForaOpacidade>0.97</ItemForaOpacidade>
			<ItemForaTonalidade>0.7</ItemForaTonalidade>
		</variables>
    </colorScheme>
	<colorScheme name="blue">
		<variables>
			<CorFundoTopo>1c2c68</CorFundoTopo>
			<CorFundoRodape>121258</CorFundoRodape>
			<CorFundoBolinhas>233688</CorFundoBolinhas>
			<CorLetrasGeral>5c5a91</CorLetrasGeral>
			<CorLetrasTitulos>f0eff1</CorLetrasTitulos>
			<FundoOpacidade>0.15</FundoOpacidade>
			<FundoSaturacao>0.5</FundoSaturacao>
			<CorMiniFundo>f0eff1</CorMiniFundo>
			<CorHelpFundo>ffffff44</CorHelpFundo>
			<CorHelpFundo2>d1cdda44</CorHelpFundo2>
			<CorDeFato>blue</CorDeFato>
			<ImagemSeletor>./_inc/images/grid-seletor-dark.png</ImagemSeletor>
			<ItemForaOpacidade>0.97</ItemForaOpacidade>
			<ItemForaTonalidade>0.7</ItemForaTonalidade>
		</variables>
    </colorScheme>
	<colorScheme name="green">
		<variables>
			<CorFundoTopo>1b5e20</CorFundoTopo>
			<CorFundoRodape>0d3b13</CorFundoRodape>
			<CorFundoBolinhas>2e7d32</CorFundoBolinhas>
			<CorLetrasGeral>6d8f6d</CorLetrasGeral>
			<CorLetrasTitulos>e8f5e9</CorLetrasTitulos>
			<FundoOpacidade>0.15</FundoOpacidade>
			<FundoSaturacao>0.6</FundoSaturacao>
			<CorMiniFundo>e8f5e9</CorMiniFundo>
			<CorHelpFundo>ffffff44</CorHelpFundo>
			<CorHelpFundo2>d1cdda44</CorHelpFundo2>
			<CorDeFato>green</CorDeFato>
			<ImagemSeletor>./_inc/images/grid-seletor-dark.png</ImagemSeletor>
			<ItemForaOpacidade>0.97</ItemForaOpacidade>
			<ItemForaTonalidade>0.7</ItemForaTonalidade>
		</variables>
    </colorScheme>
	<colorScheme name="teal">
		<variables>
			<CorFundoTopo>00695c</CorFundoTopo>
			<CorFundoRodape>004d40</CorFundoRodape>
			<CorFundoBolinhas>00796b</CorFundoBolinhas>
			<CorLetrasGeral>5e8f88</CorLetrasGeral>
			<CorLetrasTitulos>e0f2f1</CorLetrasTitulos>
			<FundoOpacidade>0.15</FundoOpacidade>
			<FundoSaturacao>0.55</FundoSaturacao>
			<CorMiniFundo>e0f2f1</CorMiniFundo>
			<CorHelpFundo>ffffff44</CorHelpFundo>
			<CorHelpFundo2>d1cdda44</CorHelpFundo2>
			<CorDeFato>teal</CorDeFato>
			<ImagemSeletor>./_inc/images/grid-seletor-dark.png</ImagemSeletor>
			<ItemForaOpacidade>0.97</ItemForaOpacidade>
			<ItemForaTonalidade>0.7</ItemForaTonalidade>
		</variables>
    </colorScheme>

	<!--///	Todas as Views ///-->
	<view name="system,gamelist">
		<image name="system-background">
			<origin>0 0</origin>
			<pos>0 0</pos>
			<size>1 1</size>
			<path>${spacerImage}</path>
			<tile>true</tile>
			<color>${CorFundoTopo}</color>
			<colorEnd>${CorFundoRodape}</colorEnd>
			<gradientType>vertical</gradientType>
			<zIndex>1</zIndex>
		</image>
		<image name="reloginho">
			<origin>1 0</origin>
			<path>./_inc/images/${CorDeFato}/reloginho.png</path>
			<zIndex>5</zIndex>
			<interpolation>linear</interpolation>
			<color>ffffffee</color>
		</image>
		<image name="setinha-com-ou-sem-controle">
			<origin>0 0.5</origin>
			<zIndex>6</zIndex>
			<color>${iiSUColor1}</color>
			<colorEnd>${iiSUColor2}</colorEnd>
			<gradientType>vertical</gradientType>
			<interpolation>linear</interpolation>
		</image>
		<image name="setinha-com-ou-sem-controle-fundo">
			<origin>0 0.5</origin>
			<zIndex>5</zIndex>
			<color>${CorMiniFundo}</color>
			<interpolation>linear</interpolation>
		</image>
		<image name="setinha-com-ou-sem-controle-sombra">
			<origin>0 0.5</origin>
			<zIndex>4</zIndex>
			<color>00000033</color>
			<interpolation>linear</interpolation>
		</image>
		<image name="setinha-apenas,setinha-apenas-sombra">
			<path>./_inc/images/setinha-sem-controle.png</path>
		</image>
		<image name="setinha-apenas">
			<origin>0 0.5</origin>
			<zIndex>5</zIndex>
			<color>${CorLetrasTitulos}</color>
			<interpolation>linear</interpolation>
		</image>
		<image name="setinha-apenas-sombra">
			<origin>0 0.5</origin>
			<zIndex>4</zIndex>
			<color>00000033</color>
			<interpolation>linear</interpolation>
		</image>
		<helpsystem name="help-esquerdo,help-direito">
			<fontSize>0.026</fontSize>
			<fontPath>${fontMedium}</fontPath>
			<letterCase>capitalize</letterCase>
			<iconColor>${CorLetrasTitulos}</iconColor>
			<textColor>${CorLetrasTitulos}</textColor>
			<scope>view</scope>
			<backgroundColor>${CorHelpFundo}</backgroundColor>
			<backgroundColorEnd>${CorHelpFundo2}</backgroundColorEnd>
			<backgroundGradientType>vertical</backgroundGradientType>
			<backgroundVerticalPadding>0.02 0.02</backgroundVerticalPadding>
		</helpsystem>
		<helpsystem name="help-esquerdo">
			<origin>0 0</origin>
		</helpsystem>
		<helpsystem name="help-direito">
			<origin>1 0</origin>
		</helpsystem>
		<helpsystem name="help-menu">
			<origin>1 0</origin>
			<pos>0.975 0.925</pos>
			<fontSize>0.026</fontSize>
			<fontSizeDimmed>0.026</fontSizeDimmed>
			<fontPath>${fontMedium}</fontPath>
			<letterCase>capitalize</letterCase>
			<iconColor>${CorLetrasGeral}</iconColor>
			<textColor>${CorLetrasGeral}</textColor>
			<originDimmed>0.5 0</originDimmed>
			<posDimmed>0.5 0.94</posDimmed>
			<textColorDimmed>${CorLetrasTitulos}</textColorDimmed>
			<iconColorDimmed>${CorLetrasTitulos}</iconColorDimmed>
			<opacityDimmed>1.0</opacityDimmed>
			<scope>menu</scope>
			<entries>start,a,b</entries>
		</helpsystem>
		<systemstatus name="system-status">
			<fontPath>${fontMedium}</fontPath>
			<height>0.042</height>
			<color>${CorLetrasTitulos}</color>
			<customIcon icon="icon_bluetooth">./_inc/images/icon-bt.svg</customIcon>
			<customIcon icon="icon_wifi">./_inc/images/icon-wifi.svg</customIcon>
			<customIcon icon="icon_cellular">./_inc/images/icon-cellular.svg</customIcon>
			<textRelativeScale>0.625</textRelativeScale>
			<scope>view</scope>
		</systemstatus>
		<clock name="clock">
			<fontPath>${fontMedium}</fontPath>
			<fontSize>0.037</fontSize>
			<color>${CorLetrasTitulos}</color>
			<horizontalAlignment>center</horizontalAlignment>
			<scope>view</scope>
		</clock>
	</view>
	<view name="system">
		<image name="system-background">
			<stationary>always</stationary>
		</image>
	</view>
	<view name="gamelist">
		<image name="system-background">
			<renderDuringTransitions>false</renderDuringTransitions>
		</image>
	</view>

	<!--/// System View ///-->
	<view name="system">
		<image name="system-background-bolinhas">
			<origin>0 0</origin>
			<pos>0 0</pos>
			<size>1 1</size>
			<path>./_inc/images/fundo_bolinhas.png</path>
			<tile>true</tile>
			<color>${CorFundoBolinhas}</color>
			<zIndex>2</zIndex>
		</image>
		<carousel name="system-carousel">
			<origin>0.5 0.5</origin>
			<size>0.7 1</size>
			<type>vertical</type>
			<unfocusedItemOpacity>0.9</unfocusedItemOpacity>
			<unfocusedItemDimming>0.9</unfocusedItemDimming>
			<itemScale>2</itemScale>
			<selectedItemOffset>0.09 0</selectedItemOffset>
			<defaultImage>${fallbackSystem}</defaultImage>
			<staticImage>${artworkFolder}</staticImage>
			<imageCornerRadius>0.008</imageCornerRadius>
			<fastScrolling>true</fastScrolling>
			<color>00000000</color>
			<textColor>${CorLetrasTitulos}</textColor>
			<textBackgroundColor>33333340</textBackgroundColor>
			<fontSize>0.045</fontSize>
			<maxItemCount>5</maxItemCount>
			<selectedItemMargins>0.11 0.11</selectedItemMargins>
		</carousel>
		<image name="setinha-com-ou-sem-controle">
			<path>./_inc/images/new-controle-borda.png</path>
		</image>
		<image name="setinha-com-ou-sem-controle-fundo">
			<path>./_inc/images/new-controle-fundo.png</path>
		</image>
		<image name="setinha-com-ou-sem-controle-sombra">
			<path>./_inc/images/new-controle-borda.png</path>
		</image>
		<text name="system-nome,system-nome-sombra,auto-nome,auto-nome-sombra,custom-nome,custom-nome-sombra">
			<origin>0 1</origin>
			<size>0.45 0.2</size>
			<fontSize>0.065</fontSize>
			<lineSpacing>1.1</lineSpacing>
			<horizontalAlignment>left</horizontalAlignment>
			<verticalAlignment>center</verticalAlignment>
			<fontPath>${fontMedium}</fontPath>
		</text>
		<text name="system-nome,system-nome-sombra">
			<text>${system.fullName.noCollections}</text>
            <letterCase>none</letterCase>
		</text>
		<text name="system-nome">
			<color>${CorLetrasTitulos}</color>
		</text>
		<text name="system-nome-sombra">
			<color>00000033</color>
			<zIndex>5</zIndex>
		</text>
		<text name="auto-nome,auto-nome-sombra">
			<text>${system.fullName.autoCollections}</text>
            <letterCase>capitalize</letterCase>
		</text>
		<text name="auto-nome">
			<color>${CorLetrasTitulos}</color>
		</text>
		<text name="auto-nome-sombra">
			<color>00000033</color>
			<zIndex>5</zIndex>
		</text>
		<text name="custom-nome,custom-nome-sombra">
			<text>${system.fullName.customCollections}</text>
            <letterCase>none</letterCase>
		</text>
		<text name="custom-nome">
			<color>${CorLetrasTitulos}</color>
		</text>
		<text name="custom-nome-sombra">
			<color>00000033</color>
			<zIndex>5</zIndex>
		</text>
		<text name="system-game-count">
			<origin>0.5 0.5</origin>
			<fontSize>0.03</fontSize> 
			<fontPath>${fontMedium}</fontPath>
			<systemdata>gamecountGamesNoText</systemdata>
			<color>${CorLetrasGeral}</color>
		</text>
		<image name="jogos-totais-fundo">
			<origin>0 0.5</origin>
			<zIndex>5</zIndex>
			<path>./_inc/images/${CorDeFato}/contador-jogos.png</path>
			<interpolation>linear</interpolation>
		</image>
		<text name="jogos-totais-indicacao">
			<origin>0.5 0.5</origin>
			<size>0.45 0.2</size>
			<fontSize>0.026</fontSize>
			<lineSpacing>1.1</lineSpacing>
			<horizontalAlignment>center</horizontalAlignment>
			<verticalAlignment>center</verticalAlignment>
			<fontPath>${fontMedium}</fontPath>
			<text>${langLabelTotalGames}</text>
			<color>${CorMiniFundo}</color>
		</text>
		<image name="system-console">
			<origin>0.5 0.5</origin>
			<pos>0.85 0.68</pos>
			<maxSize>0.25 0.4</maxSize>
			<path>./_inc/systems/controllers/${system.theme}.webp</path>
			<default>./_inc/systems/controllers/_default.webp</default>
			<interpolation>linear</interpolation>
		</image>
		<helpsystem name="help-esquerdo">
			<entries>start</entries>
		</helpsystem>
		<helpsystem name="help-direito">
			<entries>a</entries>
		</helpsystem>
	</view>
   
	<!--///	Gamelist View: Carousel	///-->
	<view name="gamelist">
		<image name="background-art">
			<origin>0 0</origin>
			<pos>0 0</pos>
			<cropSize>1 1</cropSize>
			<imageType>fanart, screenshot</imageType>
			<zIndex>2</zIndex>
			<scrollFadeIn>true</scrollFadeIn>
			<opacity>${FundoOpacidade}</opacity>
			<saturation>${FundoSaturacao}</saturation>
		</image>

		<image name="system-icon">
			<path>${artworkFolder}</path>
			<default>${fallbackSystem}</default>
			<origin>0.5 0.5</origin>
			<interpolation>linear</interpolation>
		</image>
		<image name="system-icon-sombrinha">
			<path>${spacerImage}</path>
			<origin>0.5 0.5</origin>
			<tile>true</tile>
			<color>00000033</color>
			<zIndex>4</zIndex>
		</image>
		<text name="carousel-name,carousel-name-sombra">
			<lineSpacing>1.1</lineSpacing>
			<verticalAlignment>center</verticalAlignment>
			<fontPath>${fontMedium}</fontPath>
			<metadata>name</metadata>
		</text>
		
		<text name="carousel-name-sombra">
			<color>00000033</color>
			<zIndex>5</zIndex>
		</text>
		<image name="fundo-titulo-roxo">
			<zIndex>4</zIndex>
			<origin>0 0.5</origin>
			<metadataElement>true</metadataElement>
			<interpolation>linear</interpolation>
		</image>
		<datetime name="textlist-releasedate,textlist-lastplayed">
			<origin>0 0.5</origin>
			<size>0.45 0.16</size>
			<fontSize>0.026</fontSize>
			<fontPath>${fontMedium}</fontPath>
			<horizontalAlignment>left</horizontalAlignment>
			<verticalAlignment>top</verticalAlignment>
			<color>${CorLetrasGeral}</color>
		</datetime>
		<text name="textlist-players,textlist-developer,textlist-playtime">
			<origin>0 0.5</origin>
			<fontSize>0.026</fontSize>
			<fontPath>${fontMedium}</fontPath>
			<horizontalAlignment>left</horizontalAlignment>
			<verticalAlignment>top</verticalAlignment>
			<metadataElement>true</metadataElement>
			<color>${CorLetrasGeral}</color>
		</text>
		<datetime name="textlist-releasedate">
			<metadata>releasedate</metadata>
			<format>%d/%m/%Y</format>
		</datetime>
		<text name="textlist-players">
			<size>0.45 0.16</size>
			<metadata>players</metadata>
			<defaultValue>??</defaultValue>
		</text>
		<text name="textlist-developer">
			<metadata>developer</metadata>
			<container>true</container>
			<containerType>horizontal</containerType>
		</text>
		<datetime name="textlist-lastplayed">
			<metadata>lastplayed</metadata>
		</datetime>
		<text name="textlist-playtime">
			<size>0.45 0.16</size>
			<metadata>playtime</metadata>
		</text>
		<video name="textlist-video">
			<origin>0.5 0.5</origin>
			<videoCornerRadius>0.01</videoCornerRadius>
			<imageCornerRadius>0.01</imageCornerRadius>
			<imageType>screenshot</imageType>
			<delay>0</delay>
			<pillarboxes>false</pillarboxes>
		</video>
		<image name="textlist-video-sombrinha">
			<path>${spacerImage}</path>
			<origin>0.5 0.5</origin>
			<tile>true</tile>
			<cornerRadius>0.008</cornerRadius>
			<color>00000033</color>
			<metadataElement>true</metadataElement>
			<zIndex>4</zIndex>
		</image>
		<image name="metadados-fundo">
			<zIndex>5</zIndex>
			<origin>0 0.5</origin>
			<metadataElement>true</metadataElement>
			<path>./_inc/images/${CorDeFato}/detalhes.png</path>
			<interpolation>linear</interpolation>
		</image>
		<badges name="carousel-badges">
			<origin>0.5 0</origin>
			<size>1 0.026</size>
			<horizontalAlignment>center</horizontalAlignment>
			<itemMargin>0.002 -1</itemMargin>
			<lines>1</lines>
			<itemsPerLine>9</itemsPerLine>
			<slots>all</slots>
			<customBadgeIcon badge="folder">./_inc/images/badge-folder.svg</customBadgeIcon>
			<customBadgeIcon badge="broken">./_inc/images/badge-broken.svg</customBadgeIcon>
			<customBadgeIcon badge="completed">./_inc/images/badge-completed.svg</customBadgeIcon>
			<customBadgeIcon badge="favorite">./_inc/images/badge-favorite.svg</customBadgeIcon>
			<customBadgeIcon badge="kidgame">./_inc/images/badge-kidgame.svg</customBadgeIcon>
			<customBadgeIcon badge="collection">./_inc/images/badge-collection.svg</customBadgeIcon>
			<customBadgeIcon badge="controller">./_inc/images/badge-controller.svg</customBadgeIcon>
			<customBadgeIcon badge="altemulator">./_inc/images/badge-altemulator.svg</customBadgeIcon>
			<customBadgeIcon badge="manual">./_inc/images/badge-manual.svg</customBadgeIcon>
			<badgeIconColor>${CorLetrasTitulos}</badgeIconColor>
			<controllerSize>0.8</controllerSize>
			<folderLinkSize>0.8</folderLinkSize>
		</badges>
		<helpsystem name="help-esquerdo">
			<entries>b</entries>
		</helpsystem>
		<helpsystem name="help-direito">
			<entries>start,back,x,y,a</entries>
		</helpsystem>
		
		<text name="custom-titulo">
			<fontPath>${fontMedium}</fontPath>
			<fontSize>0.02</fontSize>
			<color>${CorLetrasTitulos}</color>
			<horizontalAlignment>center</horizontalAlignment>
		</text>
	</view>

	<variant name="gamelist-carousel-default,gamelist-carousel-noicons,gamelist-carousel-noclock,gamelist-carousel-neither">
		<view name="gamelist">
			<carousel name="game-carousel">
				<origin>0.5 0.5</origin>
				<size>0.7 1</size>
				<type>vertical</type>
				<unfocusedItemOpacity>0.9</unfocusedItemOpacity>
				<unfocusedItemDimming>0.9</unfocusedItemDimming>
				<imageType>cover</imageType>
				<imageCornerRadius>0.008</imageCornerRadius>
				<defaultImage>${fallbackGame}</defaultImage>
				<defaultFolderImage>${fallbackSystem}</defaultFolderImage>
				<fastScrolling>true</fastScrolling>
				<color>00000000</color>
				<textColor>${CorLetrasTitulos}</textColor>
				<textBackgroundColor>33333340</textBackgroundColor>
				<fontSize>0.03</fontSize>
				<itemScale>2</itemScale>
				<maxItemCount>5</maxItemCount>
				<selectedItemMargins>0.11 0.11</selectedItemMargins>
				<selectedItemOffset>0.09 0</selectedItemOffset>
				<fastScrolling>true</fastScrolling>
				<imageInterpolation>nearest</imageInterpolation>
			</carousel>
			<text name="carousel-name,carousel-name-custom">
				<color>${CorLetrasTitulos}</color>
			</text>
		</view>
	</variant>
	
	<variant name="grid">
		<view name="gamelist">
			<grid name="game-grid">
				<origin>0 0</origin>
				<imageRelativeScale>0.9</imageRelativeScale>				
				<imageType>cover</imageType>
				<defaultImage>${fallbackGame}</defaultImage>
				<defaultFolderImage>${fallbackSystem}</defaultFolderImage>
				<itemScale>1.05</itemScale>
				<itemSpacing>0.00625 0</itemSpacing>
				<unfocusedItemOpacity>${ItemForaOpacidade}</unfocusedItemOpacity>
				<unfocusedItemDimming>${ItemForaTonalidade}</unfocusedItemDimming>
				<fontSize>0.03</fontSize>
